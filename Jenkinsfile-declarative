pipeline {
  agent { label 'java8_krishna' }
  stages {
    stage('vcs') {
      steps {
        git url: 'https://github.com/krishnareddy219/spring-petclinic.git',
            branch: 'develop'
        
            } 
    }
    stage('build') {
      steps {
        sh './mvnw package'
            }
          }
    stage('copy the file') {
      steps {
        sh 'sudo cp ${WORKSPACE}/target/spring-petclinic-3.0.0-SNAPSHOT.jar  /home/ubuntu/spc'
            }
          }
    
   
     } 

}
